      "targets": [
        {
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH LatestPredictions AS (\n    SELECT DISTINCT ON (p.airline, p.source_city, p.destination_city, p.travel_class, p.stops, p.departure_time, p.arrival_time, p.days_left)\n        p.airline,\n        p.source_city,\n        p.destination_city,\n        p.travel_class,\n        p.stops,\n        p.departure_time,\n        p.arrival_time,\n        p.days_left,\n        p.predicted_price,\n        t.price AS training_price,\n        ROUND(ABS(p.predicted_price::numeric - t.price::numeric), 2) AS absolute_error\n    FROM predictions p\n    JOIN flight_data t \n        ON p.airline = t.airline\n        AND p.source_city = t.source_city\n        AND p.destination_city = t.destination_city\n        AND p.travel_class = t.travel_class\n        AND p.stops = t.stops\n        AND p.departure_time = t.departure_time\n        AND p.arrival_time = t.arrival_time\n        AND p.days_left = t.days_left::INTEGER\n        AND p.duration = t.duration::FLOAT\n    WHERE p.prediction_time >= NOW() - INTERVAL '60 minutes'\n    ORDER BY p.airline, p.source_city, p.destination_city, p.travel_class, p.stops, p.departure_time, p.arrival_time, p.days_left, p.prediction_time DESC\n)\nSELECT \n    airline,\n    source_city || ' to ' || destination_city AS route,\n    travel_class,\n    stops,\n    departure_time,\n    arrival_time,\n    training_price::INTEGER,\n    predicted_price,\n    absolute_error\nFROM LatestPredictions\nORDER BY absolute_error DESC\nLimit 5;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Absolute Error in Flight Price Predictions (Top 5 Routes Effected)",
      "type": "barchart"

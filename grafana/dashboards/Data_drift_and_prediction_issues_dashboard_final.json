        {
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH all_airlines AS (\n  SELECT DISTINCT airline FROM flight_data\n  UNION\n  SELECT DISTINCT airline FROM predictions\n),\ntraining_counts AS (\n  SELECT airline, COUNT(*) AS count\n  FROM flight_data\n  GROUP BY airline\n),\nserving_counts AS (\n  SELECT airline, COUNT(*) AS count\n  FROM predictions\n  GROUP BY airline\n),\ntotal_counts AS (\n  SELECT (SELECT COUNT(*) FROM flight_data) AS training_total,\n         (SELECT COUNT(*) FROM predictions) AS serving_total\n),\ncombined AS (\n  SELECT a.airline,\n         COALESCE(t.count, 0) AS training_count,\n         COALESCE(s.count, 0) AS serving_count,\n         total_counts.training_total,\n         total_counts.serving_total\n  FROM all_airlines a\n  LEFT JOIN training_counts t ON a.airline = t.airline\n  LEFT JOIN serving_counts s ON a.airline = s.airline\n  CROSS JOIN total_counts\n)\nSELECT airline,\n       (training_count * 100.0 / training_total) AS training_percentage,\n       (serving_count * 100.0 / serving_total) AS serving_percentage\nFROM combined\nORDER BY airline;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          }
        }
      ],
      "title": "Airline Distribution - Data Drift ",
      "type": "barchart"
